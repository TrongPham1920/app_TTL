{
  "expo": {
    "packagerOpts": {
      "hostType": "lan"
    },
    "name": "TroThaLo",
    "displayName": "TroThaLo",
    "slug": "my-new-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trongheo2013.mynewapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.trongheo2013.mynewapp",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/1.png"
    },
    "extra": {
      "eas": {
        "projectId": "812d56cb-a87f-4eab-a0ef-16b4cd9ba787"
      }
    },
    "plugins": ["expo-secure-store"]
  }
}
